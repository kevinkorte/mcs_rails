<div class="container">
  <div class="page-header">
    <h1>Machine List</h1>
  </div>
</div>
<div class="container">
  <div class="row">
    <div class="col-md-12">
      <div class="panel panel-default">
        <div class="panel-heading">Most Recent Machines</div>
        <table class="table table-striped table-hover">
          <thead>
            <tr>
              <th>Title</th>
            </tr>
          </thead>
          <tbody>
           
        </table>
      </div>

    </div>
    
  </div>
</div>

<ul>
  <% for machine in @machines %>
  <li>
    <%= machine.title %>
  </li>
  <% end %>
</ul>

<script>
  $("tr[data-link]").click(function() {
  window.location = $(this).data('link');
});
</script>